	Doi = {10.1016/j.jtbi.2006.06.027},
	Journal = {J Theor Biol},
	Keywords = {Game Theory, Evolution, Animals, Models, Biological, Humans, Interpersonal Relations, Cooperative Behavior},
	Month = dec,
	Number = {3},
	Pages = {437--43},
	Pii = {S0022-5193(06)00273-6},
	Pmid = {16901509},
	Rating = {0},
	Title = {Active linking in evolutionary games},
