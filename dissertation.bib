	Doi = {10.1016/j.jtbi.2005.10.024},
	Journal = {J Theor Biol},
	Keywords = {Humans, Game Theory, Cooperative Behavior, Population Dynamics, Models, Psychological, Evolution, Group Processes},
	Month = {Jun},
	Number = {4},
	Pages = {627--36},
