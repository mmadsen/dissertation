@article{ishida2009,
	Abstract = { Sewall Wright and Gustave Mal{\'e}cot developed important theories of isolation by distance. Wright's theory was statistical and Mal{\'e}cot's probabilistic. Because of this mathematical difference, they were not clear about the relationship between their theories. In this paper, I make two points to clarify this relationship. First, I argue that Wright's theory concerns what I call ecological isolation by distance, whereas Mal{\'e}cot's concerns what I call genetic isolation by distance. Second, I suggest that if Wright's theory is interpreted appropriately, a previously unnoticed connection between the two theories emerges. },
	Author = {Ishida, Yoichi},
