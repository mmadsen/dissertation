@article{Ohtsuki2007,
	Abstract = {We study the evolution of cooperation modeled as symmetric 2x2 games
	in a population whose structure is split into an interaction graph
	defining who plays with whom and a replacement graph specifying evolutionary
	competition. We find it is always harder for cooperators to evolve
	whenever the two graphs do not coincide. In the thermodynamic limit,
	the dynamics on both graphs is given by a replicator equation with
	a rescaled payoff matrix in a rescaled time. Analytical results are
	obtained in the pair approximation and for weak selection. Their
	validity is confirmed by computer simulations.},
	Affiliation = {Program for Evolutionary Dynamics, Harvard University, Cambridge, Massachusetts 02138, USA.},
	Author = {Hisashi Ohtsuki and Martin A Nowak and Jorge M Pacheco},
