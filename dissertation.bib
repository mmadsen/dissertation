	Language = {ENG},
	Local-Url = {file://localhost/Users/mark/Documents/Papers/2009/Kendal/Kendal2009%20Journal%20of%20Theoretical%20Biology%20The%20evolution%20of%20social%20learning.pdf},
	Month = jun,
	Pii = {S0022-5193(09)00228-8},
	Pmid = {19501102},
	Rating = {0},
	Title = {The evolution of social learning rules: Payoff-biased and frequency-dependent biased transmission},
