	Doi = {10.1007/s11538-006-9145-1},
	Journal = {Bull Math Biol},
	Keywords = {Animals, Game Theory, Altruism, Evolution, Population Dynamics, Computer Simulation},
	Month = {Feb},
	Number = {2},
	Pages = {659--76},
	Pmid = {17318678},
	Rating = {0},
	Title = {Spatial patterns of prisoner's dilemma game in metapopulations},
